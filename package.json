{
  "name": "hasura-graphql",
  "version": "1.0.0",
  "description": "Dependencies used for testing hasura-graphql",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/tsc && node hasura-graphql.export.js",
    "local": "./node_modules/.bin/json-graphql-server hasura-graphql.export.js",
    "hasura-update": "./node_modules/.bin/json2graphql https://endfront.herokuapp.com/ -s 'ForgeTech_EndFront_$1Ckness' -d hasura-graphql.export.js",
    "hasura-overwrite": "./node_modules/.bin/json2graphql https://endfront.herokuapp.com/ --overwrite -s 'ForgeTech_EndFront_$1Ckness' -d hasura-graphql.export.js",
    "faker-local": "./node_modules/.bin/graphql-faker ./model/graphql-faker.graphql --extend http://localhost:3000",
    "faker-hasura": "./node_modules/.bin/graphql-faker ./model/graphql-faker.graphql --extend https://endfront.herokuapp.com/v1alpha1/graphql --header 'x-hasura-admin-secret: ForgeTech_EndFront_$1Ckness",
    "faker": "./node_modules/.bin/graphql-faker ./model/graphql-faker.graphql",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ForgeTech/hasura-graphql.git"
  },
  "keywords": [
    "graphql",
    "hasura"
  ],
  "author": "florian neumann",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ForgeTech/hasura-graphql/issues"
  },
  "homepage": "https://github.com/ForgeTech/hasura-graphql#readme",
  "dependencies": {
    "camelcase": "^5.0.0",
    "graphql-faker": "^1.9.0",
    "graphqlviz": "^3.0.0",
    "json-graphql-server": "^2.1.3",
    "json2graphql": "^0.1.3",
    "material-colors": "^1.2.6",
    "typescript": "^3.3.3333"
  },
  "devDependencies": {
    "@types/node": "^11.9.5"
  }
}
