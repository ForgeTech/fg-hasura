{
  "name": "hasura-graphql",
  "version": "1.0.0",
  "description": "Dependencies used for testing hasura-graphql",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "postinstall": "npm run test",
    "start": "npm run build",
    "build": "./node_modules/.bin/tsc && node dist/json.export.js",
    "local": "./node_modules/.bin/json-graphql-server dist/json.export.js",
    "force-resolutions": "rm -rf node_modules && npx npm-force-resolutions && npm i",
    "swagger-to-graphql": "swagger-to-graphql --swagger=./src/swagger/powerbot-api-v0.swagger.json > ./dist/powerbot-api-v0.swagger.graphql",
    "graphql-create": "./node_modules/.bin/graphql create",
    "graphql-voyager": "./node_modules/.bin/graphql voyager",
    "docker-hasura-up": "docker-compose up -d",
    "docker-hasura-down": "docker-compose down",
    "docker-hasura-overwrite": "./node_modules/.bin/json2graphql http://localhost:8080/ -d dist/json.export.js -o",
    "docker-hasura-faker": "./node_modules/.bin/graphql-faker ./src/model/docker-hasura-faker.graphql --extend http://localhost:8080/v1alpha1/graphql --header x-hasura-admin-secret: admin",
    "docker-hasura-schema": "./node_modules/.bin/graphql get-schema --endpoint http://localhost:8080/v1alpha1/graphql --header x-hasura-admin-secret: admin --output dist/docker-hasura-schema.graphql",
    "docker-hasura-faker-schema": "./node_modules/.bin/graphql get-schema --endpoint http://localhost:9002/graphql --header x-hasura-admin-secret: admin --output dist/docker-hasura-faker-schema.graphql",
    "docker-hasura-inspect-faker": "./node_modules/.bin/graphql-inspector diff dist/docker-hasura-schema.graphql dist/docker-hasura-faker-schema.graphql",
    "heroku-hasura-schema": "./node_modules/.bin/graphql get-schema --endpoint https://endfront.herokuapp.com/v1alpha1/graphql --header x-hasura-admin-secret: admin --output dist/heroku-hasura-schema.graphql",
    "heroku-hasura-update": "./node_modules/.bin/json2graphql https://endfront.herokuapp.com/ -k ForgeTech_EndFront_$1Ckness -d dist/json.export.js",
    "heroku-hasura-overwrite": "./node_modules/.bin/json2graphql https://endfront.herokuapp.com/ -k ForgeTech_EndFront_$1Ckness -d dist/json.export.js -o",
    "heroku-hasura-faker": "./node_modules/.bin/graphql-faker ./model/heroku-hasura-faker.graphql --extend https://endfront.herokuapp.com/v1alpha1/graphql --header x-hasura-admin-secret: ForgeTech_EndFront_$1Ckness",
    "faker": "./node_modules/.bin/graphql-faker ./src/model/faker.graphql",
    "blowson": "./node_modules/.bin/blowson src/data/data.blowson.js dist/data.blowson-extended.json",
    "rover": "",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ForgeTech/hasura-graphql.git"
  },
  "keywords": [
    "graphql",
    "hasura"
  ],
  "author": "florian neumann",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ForgeTech/hasura-graphql/issues"
  },
  "homepage": "https://github.com/ForgeTech/hasura-graphql#readme",
  "dependencies": {
    "@graphql-inspector/cli": "^0.13.3",
    "@graphql-inspector/core": "^0.13.3",
    "@types/jss": "^9.5.8",
    "@types/node": "^11.9.5",
    "blee": "^1.0.1",
    "blowson": "^1.3.3",
    "camelcase": "^5.0.0",
    "countries-and-timezones": "^1.0.1",
    "country-iso": "^2.0.5",
    "country-list": "^2.1.0",
    "currency-codes": "^1.5.0",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "graphql-cli": "^3.0.11",
    "graphql-cli-voyager": "^0.1.3",
    "graphql-codegen-typescript-apollo-angular": "^0.18.0",
    "graphql-faker": "^1.9.0",
    "graphql-generator": "^1.2.0",
    "graphql-inspector": "^0.5.1",
    "graphql-voyager": "^1.0.0-rc.26",
    "graphqlviz": "^3.0.0",
    "i18n-iso-countries": "^3.7.8",
    "iso-language-codes": "^1.0.2",
    "json-graphql-server": "^2.1.3",
    "json2graphql": "^0.1.3",
    "jss": "^9.8.7",
    "known-css-properties": "^0.12.0",
    "langs": "^2.0.0",
    "language-list": "0.0.3",
    "locale-code": "^2.0.1",
    "material-colors": "^1.2.6",
    "npm-force-resolutions": "0.0.2",
    "swagger-to-graphql": "^1.4.5",
    "typescript": "^3.3.3333",
    "world-countries": "^2.1.0"
  },
  "devDependencies": {},
  "resolution": {
    "graphql": "14.0.2",
    "@types/graphql": "0.9.4"
  }
}
